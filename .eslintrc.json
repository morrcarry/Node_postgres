{
    "env": {
        "commonjs": true,
        "es2022": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "camelcase": "off",
        "no-plusplus": "off",
        "no-underscore-dangle":"off",
        "no-nested-ternary":"off",
        "no-ternary":"off",
        "no-multi-assign":"off",
        "no-prototype-builtins":"off",
        "max-len":["error",{"code":125}],
        "no-await-in-loop": "off",
        "no-restricted-syntax":["error","LabeledStatement", "WithStatement"]//enable ForOf, ForIn
    }
}
